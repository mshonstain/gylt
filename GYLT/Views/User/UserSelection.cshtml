@model IList<GYLT.Models.UserModel>

@{
	ViewBag.Title = "UserSelection";
}

<h2>Who Are You?</h2>

@for (int i = 0; i < Model.Count; i++)
{
	<div class="col-md-4">
		@Html.Partial("_UserCard", Model[i])
	</div>
}

<div class="col-md-4">
	@Html.Partial("_UserCard", new GYLT.Models.UserModel { UserID = 0, IconName = "MaleSilhouette_Gray", Username = "New User" })
</div>

